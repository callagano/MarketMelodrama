{
  "version": 2,
  "buildCommand": "bash vercel-build.sh",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["fra1"],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * *"
    }
  ],
  "env": {
    "ALPHA_VANTAGE_API_KEY": "@alpha_vantage_api_key",
    "ALPHA_VANTAGE_BASE_URL": "https://www.alphavantage.co/query"
  }
} 